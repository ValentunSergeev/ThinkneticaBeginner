<%= form_for([:admin, @railway_station]) do |f| %>
  <%= render 'common/errors', resource: @railway_station %>

  <div class="field">
    <%= f.label :title, class: 'control-label' %>
    <%= f.text_field :title %>
  </div>

  <div class="actions">
    <%= f.submit t('.submit', action: action_for(@railway_station)) %>
  </div>
<% end %>
